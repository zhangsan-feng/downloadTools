{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "downloadTools",
  "version": "0.1.0",
  "identifier": "winTools",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },

  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "resizable": false,
        "minWidth": 1600,
        "minHeight": 700,
        "center": true,
        "hiddenTitle": true,
        "contentProtected": false,
        "maximized": false,
        "maximizable": false,
        "title": "downloadTools    https://github.com/zhangsan-feng/downloadTools"
      }
    ],

    "security": {
      "csp": null
    }
  },
  "bundle": {
    "windows": {
      "wix": {
        "language": "zh-CN"
      }
    },

    "active": true,
    "targets": ["msi", "dmg"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
